{
  "name": "anyslope",
  "version": "0.0.1",
  "description": "AnySlope allows users to read any sakamichi's blogs in one application",
  "main": "app/index.js",
  "scripts": {
    "build:pug": "./node_modules/.bin/pug --hierarchy -o app/dst/ src/",
    "build:js": "./node_modules/.bin/webpack --mode production",
    "build": "npm-run-all build:*",
    "watch:pug": "npm run build:pug -- -w",
    "watch:js": "./node_modules/.bin/webpack-dev-server --mode development",
    "watch:start": "NODE_ENV=development yarn start",
    "watch": "npm-run-all --parallel watch:*",
    "start": "electron .",
    "postinstall": "npm rebuild",
    "package": "./node_modules/.bin/electron-packager . anyslope --platform=darwin --arch=x64 --electron-version=3.0.10"
  },
  "keywords": [],
  "author": "Hiroki Usuba <hrky0206@gmail.com> (http://mimorisuzuko.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@types/electron": "^1.6.10",
    "autobind-decorator": "^2.4.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "brace": "^0.11.1",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^13.1.0",
    "emotion": "^10.0.7",
    "eslint": "^5.14.1",
    "eslint-plugin-react": "^7.12.4",
    "fs-extra": "^7.0.1",
    "hls.js": "^0.12.4",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "marked": "^0.6.1",
    "node-sass": "^4.11.0",
